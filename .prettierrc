{
    "bracketSpacing": true,
    "tabWidth": 4,
    "bracketSameLine": true,
    "singleQuote": false,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "semi": true,
    "printWidth": 110,
    "arrowParens": "always",
    "importOrder": ["^@components/(.*)", "^@utils/(.*)", "^~/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"]
}
